{
    "version": 2,
    "devCommand": "uvicorn inventory_app.main:app --reload --port 8080",
    "builds": [
        {
            "src": "build_files.sh",
            "use": "@vercel/python"            
        }
          
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "inventory_app/main.py"
        }
    ]
}