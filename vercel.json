{
    "version": 4,
    "builds": [
        {
            "src": "inventory_app/main.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.10.11" }
        }
        ,{
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
              "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "inventory_app/main.py"
        }
    ]
}